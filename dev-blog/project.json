{
  "name": "dev-blog",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "dev-blog",
  "tags": [],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/_site"],
      "options": {
        "command": "bundle exec jekyll build --baseurl https://monday-sun.github.io"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bundle exec jekyll serve"
      }
    }
  }
}
