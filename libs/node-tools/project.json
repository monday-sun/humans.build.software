{
  "name": "node-tools",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/node-tools/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "install": {
      "executor": "@nx/node:package-manager",
      "options": {
        "command": "install",
        "packageManager": "yarn",
        "workingDirectory": "libs/node-tools"
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/node-tools",
        "main": "libs/node-tools/src/index.ts",
        "tsConfig": "libs/node-tools/tsconfig.lib.json",
        "assets": ["libs/node-tools/*.md"]
      }
    }
  }
}
